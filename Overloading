package overloading;

public class MethodOverloading {
	
	public int addition (int a, int b) {
		return a+b;
	}
	public float addition(float a,float b)
	{
		return a+b;
	}
	public float addition(int a,float b)
	{
		return a+b;
	}
	public float addition(float a,int b)
	{
		return a+b;
	}
package overloading;

public class OverloadingDemo {

	public static void main(String[] args) {
		MethodOverloading obj=new MethodOverloading();
		System.out.println(obj.addition(12, 12.2f));
		System.out.println(obj.addition(12.5f, 452.32f));
		System.out.println(obj.addition(12, 45));
		System.out.println(obj.addition(23.21f, 45));
		
		System.out.println("........Constructor Overloading........");
		Point p=new Point();
		System.out.println(p);
		
		Point p1=new Point(4.5f);
		System.out.println(p1);
		
		Point p2=new Point(2.3f,8.9f);
		System.out.println(p2);

	}

}

}
